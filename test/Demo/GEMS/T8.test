<?xml version="1.1" encoding="ASCII"?>
<TestUnit:TestContainerType xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:CertificationCommands="com.esprow.nozomi.testunit.commands.certification" xmlns:TestUnit="com.esprow.nozomi.testunit" formatVersion="2.1">
  <test comment="As part of this test, the counterparty has to submit a NewOrderSingle (message type D) limit order for a given number of shares of a specific instrument at a given price.&#xD;&#xA;Subsequently, ExecutionReports (message type 8) are returned with the order acknowledgement, 2 partial fills, and finally a fill." loggingLevel="TRACE" certificationUnit="true" dictionary="Quick Fix 4.2" implementsBridgeMode="true">
    <testCommand xsi:type="CertificationCommands:CreateMessageRelayCommand" targetSystemId="%CONFIGURATIONS.CERTIFICATION.SYSTEM%" targetHandlerId="%CONFIGURATIONS.CERTIFICATION.DESTINATION%" forwardSystemId="%CONFIGURATIONS.CERTIFICATION.BRIDGE.SYSTEM%" forwardHandlerId="%CONFIGURATIONS.CERTIFICATION.BRIDGE.DESTINATION%" messageTypeFilters="" filterShowOnly="true" validateMessages="true"/>
    <testCommand xsi:type="TestUnit:GetInteractiveFeedbackCommand">
      <instructionMultiLanguage key="en" value="Please enter a new limit order to buy (B) 10000 shares of ABC at price 100 HKD."/>
    </testCommand>
    <testCommand xsi:type="TestUnit:WaitCommand" messageId="incomingLimitOrder" messageSenderProducer="//@test/@testCommand.0" timeout="30" searchFromLastMatch="true">
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="HandlInst" value="1" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Symbol" value="ABC"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Side" value="1" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="OrderQty" value="10000" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="OrdType" value="2" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Price" value="100" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="StandardHeader.MsgType" value="D"/>
    </testCommand>
    <testCommand xsi:type="TestUnit:WaitCommand" messageSenderProducer="//@test/@testCommand.0" timeout="-1" searchFromLastMatch="true" outgoingMessage="true" removeMatched="true">
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Price" value="100" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="ExecType" value="0" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Symbol" value="@tagFromLastIncomingMessage(55, 1, D)@"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="OrderQty" value="10000" checkType="GREATER_OR_EQUAL" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="LeavesQty" value="%MESSAGES.incomingLimitOrder.OrderQty%" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="LastPx" value="@tagFromLastOutgoingMessage(44, 1, 8)@" checkType="EXISTS" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="35" value="9" fieldReferenceType="TAG"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Side" value="%MESSAGES.incomingLimitOrder.Side%" fieldType="Char"/>
    </testCommand>
  </test>
</TestUnit:TestContainerType>
