<?xml version="1.1" encoding="ASCII"?>
<TestUnit:TestContainerType xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:TestUnit="com.esprow.nozomi.testunit" formatVersion="2.1">
  <test comment="As part of this test, the counterparty has to submit a NewOrderSingle (message type D) limit order for a given number of shares of a specific instrument at a given price.&#xD;&#xA;Subsequently, ExecutionReports (message type 8) are returned with the order acknowledgement, 2 partial fills, and finally a fill." loggingLevel="TRACE" certificationUnit="true" dictionary="Quick Fix 4.2">
    <testCommand xsi:type="TestUnit:NewVar" comment="Function to get current date and time" variableId="Current Timestamp" value="currentDateTime()"/>
    <testCommand xsi:type="TestUnit:LogEntry" comment="" text="Current Timestamp = %VARIABLES.Current Timestamp%"/>
    <testCommand xsi:type="TestUnit:NewVar" comment="Function to convert timestamp to specified format and timezone&#xD;&#xA;&#xD;&#xA;Format:&#xD;&#xA;convertDateTime(String inputDateTime, String inputFormat, String outputFormat, String outputTimeZone)" variableId="ConvertDateTime" value="convertDateTime(&quot;%VARIABLES.Current Timestamp%&quot;, &quot;yyyyMMdd-HH:mm:ss.SSS&quot;, &quot;yyyyMMdd-HH:mm&quot;, &quot;Asia/Singapore&quot;)"/>
    <testCommand xsi:type="TestUnit:LogEntry" comment="" text="Timestamp after conversion = %VARIABLES.ConvertDateTime%"/>
    <testCommand xsi:type="TestUnit:GetInteractiveFeedbackCommand" variableId="TimeStamp" requireUserInput="true">
      <instructionMultiLanguage key="en" value="Please check the timestamp and timezone if it's displayed in SGP timezone?"/>
    </testCommand>
  </test>
</TestUnit:TestContainerType>
