<?xml version="1.1" encoding="ASCII"?>
<TestUnit:TestContainerType xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ExchangeDictionary="com.esprow.nozomi.dictionary.exchange" xmlns:TestUnit="com.esprow.nozomi.testunit" formatVersion="2.1">
  <test>
    <testCommand xsi:type="TestUnit:NewMessageSender" messageHandlerId="sender" targetSystemId="Cert" targetHandlerId="SGX ITCH Relay 100" autoConnect="true" collectStatistics="false" sendSync="true" override="true" validateMessages="true" cleanQueue="true" disableDictionaryMismatchCheck="false"/>
    <testCommand xsi:type="TestUnit:CreateAndSendMessage" messageId="Login" messageSenderProducer="//@test/@testCommand.0">
      <message xsi:type="ExchangeDictionary:ExchangeMessage" name="SoupBinTCP_Login_Request" dictionaryName="SGX_Glimpse+ITCH" msgType="SoupBinTCP_L">
        <content xsi:type="ExchangeDictionary:ExchangeComponent" name="StandardHeader">
          <content xsi:type="ExchangeDictionary:ExchangeGroup" name="Header">
            <content xsi:type="ExchangeDictionary:ExchangeCharField" name="Message_Type" required="true" length="1" padding="LEFT"/>
          </content>
          <content xsi:type="ExchangeDictionary:ExchangeGroup" name="SoupBinTCP_Header">
            <content xsi:type="ExchangeDictionary:ExchangeUintField" name="SoupBinTCP_Packet_Length" required="true" length="2" padding="LEFT"/>
            <content xsi:type="ExchangeDictionary:ExchangeCharField" name="SoupBinTCP_Packet_Type" required="true" stringValue="L" length="1" offset="2" padding="LEFT"/>
          </content>
        </content>
        <content xsi:type="ExchangeDictionary:ExchangeStringField" name="SoupBinTCP_Username" required="true" length="6" offset="3" padding="RIGHT" paddingChar="32"/>
        <content xsi:type="ExchangeDictionary:ExchangeStringField" name="SoupBinTCP_Password" required="true" length="10" offset="9" padding="RIGHT" paddingChar="32"/>
        <content xsi:type="ExchangeDictionary:ExchangeStringField" name="SoupBinTCP_Requested_Session" required="true" length="10" offset="19" padding="LEFT" paddingChar="32"/>
        <content xsi:type="ExchangeDictionary:ExchangeStringField" name="SoupBinTCP_Requested_Sequence_Number" required="true" length="20" offset="29" padding="LEFT" paddingChar="32"/>
      </message>
    </testCommand>
    <testCommand xsi:type="TestUnit:WaitCommand" messageSenderProducer="//@test/@testCommand.0" searchFromLastMatch="true">
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="StandardHeader.SoupBinTCP_Header[].SoupBinTCP_Packet_Type" value="A" fieldType="Char"/>
    </testCommand>
  </test>
</TestUnit:TestContainerType>
