<?xml version="1.1" encoding="ASCII"?>
<TestUnit:TestContainerType xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:CertificationCommands="com.esprow.nozomi.testunit.commands.certification" xmlns:TestUnit="com.esprow.nozomi.testunit" formatVersion="2.1">
  <test comment="As part of this test, the counterparty has to submit a NewOrderSingle (message type D) limit order for a given number of shares of a specific instrument at a given price.&#xD;&#xA;Subsequently, ExecutionReports (message type 8) are returned with the order acknowledgement, 2 partial fills, and finally a fill." loggingLevel="TRACE" certificationUnit="true" dictionary="Quick Fix 4.2">
    <testCommand xsi:type="CertificationCommands:CreateCertificationReceiverCommand" targetSystemId="%CONFIGURATIONS.CERTIFICATION.SYSTEM%" targetHandlerId="%CONFIGURATIONS.CERTIFICATION.DESTINATION%" ignoreAdminMessages="true" validateMessages="true" disableDictionaryMismatchCheck="false"/>
    <testCommand xsi:type="TestUnit:GetInteractiveFeedbackCommand">
      <instructionMultiLanguage key="en" value="Please enter a new limit order to buy (B) 10000 shares of 5.HK at price 100 HKD."/>
    </testCommand>
    <testCommand xsi:type="TestUnit:WaitCommand" messageId="incomingLimitOrder" messageSenderProducer="//@test/@testCommand.0" timeout="900" searchFromLastMatch="true">
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="HandlInst" value="3" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Symbol" value="5.HK"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Side" value="1" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="OrderQty" value="10000" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="OrdType" value="2" fieldType="Char"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="Price" value="100" fieldType="Double"/>
      <testCommand xsi:type="TestUnit:CreateFieldValueCheckCommand" path="StandardHeader.MsgType" value="D"/>
    </testCommand>
    <testCommand xsi:type="TestUnit:LoadMessage" messageId="Ack">
      <messageTemplates>../../../C-Box FIX Protocol Test Suite/Messages/ExecutionReport_New.msg</messageTemplates>
    </testCommand>
    <testCommand xsi:type="TestUnit:SendMessage" messageSenderProducer="//@test/@testCommand.0" messageProducer="//@test/@testCommand.3"/>
  </test>
</TestUnit:TestContainerType>
